{
  "name": "SpringType DevTools",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "SpringType extension to inspect and debug SpringType components",
  "permissions": ["tabs","activeTab"],
  "minimum_chrome_version": "55",
  "devtools_page": "browser-action/index.html",
  "browser_action": {
    "default_icon": {
      "16": "assets/disabled/icon_16.png",
      "32": "assets/disabled/icon_32.png",
      "48": "assets/disabled/icon_48.png",
      "128": "assets/disabled/icon_128.png"
    }
  },
  "icons": {
    "16": "assets/activated/icon_16.png",
    "32": "assets/activated/icon_32.png",
    "48": "assets/activated/icon_48.png",
    "128": "assets/activated/icon_128.png"
  }
}
